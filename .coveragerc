[run]
omit =
    .pytest_cache
    run.py
    manage.py
    venv/*
    ../venv/*
    ../../venv/*
    ../../../venv/*

[report]
omit =
    .pytest_cache
    run.py
    manage.py
    venv/*
    ../venv/*
    ../../venv/*
    ../../../venv/*


ignore_errors = True

[html]
directory = coverage_html_report
